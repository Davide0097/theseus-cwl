{
  "name": "@theseus-cwl/configurations",
  "description": "Collection of internal static configurations.",
  "version": "1.1.0",
  "author": "davide giorgiutti",
  "license": "ISC",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "build": "tsup src/index.ts --format cjs,esm --dts"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@theseus-cwl/typescript-config": "*",
    "typescript": "^5.8.2",
    "tsup": "^8.5.0"
  }
}